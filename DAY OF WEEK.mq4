//--- switch day of week

   switch(TimeDayOfWeek(TimeCurrent()))
     {
      case 0:// Sunday

         break;

      case 1:// Monday

         break;

      case 2:// Tuesday

         break;

      case 3:// Wednesday

         break;

      case 4:// Thursday

         break;

      case 5:// Friday

         break;

      case 6:// Saturday

         break;
     }
